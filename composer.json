{
	"name": "flarum-lang/armenian",
	"type": "flarum-extension",
	"description": "Armenian language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/armenian",
	"keywords": [
		"locale",
		"flarum",
		"flarum-locale",
		"hy-locale",
		"armenian-locale",
		"armenian"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/flarum-lang/armenian/issues",
		"source": "https://github.com/flarum-lang/armenian"
	},
	"require": {
		"flarum/core": "^1.6"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"flarum-extension": {
			"title": "Armenian",
			"category": "language",
			"icon": {
				"image": "icon.svg",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "hy",
			"title": "Armenian"
		},
		"extiverse": {
			"discuss": ""
		}
	}
}
